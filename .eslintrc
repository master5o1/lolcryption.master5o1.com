{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 1
    },
    "ecmaFeatures": {
      "modules": true,
      "arrowFunctions": true
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
